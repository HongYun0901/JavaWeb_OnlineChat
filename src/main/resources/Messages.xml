<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.sshblog.entity.Messages">
        <table name="messages" schema="JavaWeb" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="text">
                <column name="text"/>
            </basic>
            <basic name="senderId">
                <column name="sender_id"/>
            </basic>
            <basic name="senderName">
                <column name="sender_name"/>
            </basic>
            <basic name="receiverId">
                <column name="receiver_id"/>
            </basic>
            <basic name="receiverName">
                <column name="receiver_name"/>
            </basic>
            <basic name="sendTime">
                <column name="send_time"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
